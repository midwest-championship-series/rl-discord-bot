{
  "name": "rl-rank-bot",
  "version": "1.0.0",
  "description": "a bot to keep ranks up to date on discord servers",
  "scripts": {
    "start": "nodemon -L --exec yarn serve:dev",
    "serve:dev": "yarn build:dev && node -r dotenv/config ./build/",
    "serve:prod": "yarn build:prod && forever ./build/",
    "build:dev": "rm -rf build && tsc && cp .env build",
    "build:prod": "rm -rf build && tsc -p tsconfig-prod.json",
    "test": "yarn build:dev && mocha 'build/**/*.spec.js' --timeout 4000 --exit",
    "test:ci": "yarn build:prod && MOCHA_FILE=./test-results/mocha/results.xml mocha 'build/**/*.spec.js' --reporter mocha-junit-reporter --timeout 4000 --exit"
  },
  "main": "./src/index.js",
  "repository": "git@github.com:corbfon/express-circleci-launcher.git",
  "author": "corbfon <corbin.fonville@gmail.com>",
  "private": true,
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/method-override": "^0.0.31",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "method-override": "^3.0.0",
    "node-schedule": "^1.3.2",
    "simple-oauth2": "^2.2.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node-schedule": "^1.2.3",
    "@types/simple-oauth2": "^2.2.1",
    "@types/sinon": "^7.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nodemon": "^1.18.9",
    "sinon": "^7.2.2"
  },
  "engines": {
    "node": "^v10.15.0"
  }
}
