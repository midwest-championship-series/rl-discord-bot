{
  "name": "rl-rank-bot",
  "version": "1.0.0",
  "description": "a bot to keep ranks up to date on discord servers",
  "scripts": {
    "start": "nodemon -L --exec node --inspect=9229 -r ts-node/register src",
    "serve:dev": "yarn build:dev && node -r dotenv/config ./build/",
    "serve:prod": "yarn build:prod && forever ./build/",
    "build:dev": "rm -rf build && tsc && cp .env build",
    "build:prod": "rm -rf build && tsc",
    "test": "yarn build:dev && mocha 'build/**/*.spec.js' --timeout 4000 --exit",
    "test:ci": "yarn build:prod && MOCHA_FILE=./test-results/mocha/results.xml mocha 'build/**/*.spec.js' --reporter mocha-junit-reporter --timeout 4000 --exit"
  },
  "main": "./src/index.js",
  "repository": "git@github.com:corbfon/express-circleci-launcher.git",
  "author": "corbfon <corbin.fonville@gmail.com>",
  "private": true,
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/method-override": "^0.0.31",
    "body-parser": "^1.18.3",
    "discord.js": "^12.0.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "method-override": "^3.0.0",
    "request": "^2.88.2",
    "simple-oauth2": "^2.2.1",
    "typescript": "^3.7.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/request": "^2.48.4",
    "@types/simple-oauth2": "^2.2.1",
    "@types/sinon": "^7.0.3",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "sinon": "^7.2.2",
    "ts-node": "^8.7.0"
  },
  "engines": {
    "node": "^v12.13.0"
  }
}
